{
  "name": "piedweb/google-spreadsheet-seo-scraper",
  "type": "library",
  "description": "Open source excel or libreoffice and PHP SEO google position SERP checker to track and follow a few website's keywords positions",
  "keywords": [
    "Pied Web",
    "Google Spreadsheet Seo Scraper"
  ],
  "homepage": "https://dev.piedweb.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin D. (ak Pied Web)",
      "email": "contact@robin-d.fr",
      "homepage": "https://piedweb.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "ropendev/google": "^0",
    "symfony/console": "^5",
    "league/csv": "^9.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "squizlabs/php_codesniffer": "^3.0",
    "symfony/var-dumper": "^5.2",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "PiedWeb\\GoogleSpreadsheetSeoScraper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PiedWeb\\GoogleSpreadsheetSeoScraper\\": "tests"
    }
  },
  "scripts": {
    "format": "./vendor/bin/php-cs-fixer fix src"
  }
}
